
var lat = parseFloat(datasources["lat"]["field1"]);
var lng = parseFloat(datasources["lng"]["field2"]);
var time = String(datasources["lat"]["created_at"]);
return `<html>
<body>
<link rel="stylesheet" href="https://unpkg.com/leaflet@1.5.1/dist/leaflet.css"
   integrity="sha512-xwE/Az9zrjBIphAcBb3F6JVqxf46+CDLwfLMHloNu6KEQCAWi6HcDUbeOfBIptF7tcCzusKFjFw2yuvEpDL9wQ=="
   crossorigin=""/>
<script src="https://unpkg.com/leaflet@1.5.1/dist/leaflet.js"
   integrity="sha512-GffPMF3RvMeYyc1LWMHtK8EbPv0iNZ8/oTtHPx9/cc2ILxQ+u905qIwdpULaqDkyBKgOaB57QTMg7ztg8Jm2Og=="
   crossorigin=""></script>
<style>
#mapid { height: 600px;
width: 600px;}
</style>
<p> Position retrieved at `+time+`
<div id="mapid"></div>

</body>
<script>
var mymap = L.map('mapid').setView([`+lat+`, `+lng+`], 16);
L.tileLayer('https://api.tiles.mapbox.com/v4/{id}/{z}/{x}/{y}.png?access_token=pk.eyJ1IjoicnJyYWpwdXQiLCJhIjoiY2p6OGVvZGlnMGwyYTNtbnlrNm9iN2ptMCJ9.cFzS8HLWt7sWIF_3sNci4Q', {
    attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/">OpenStreetMap</a> contributors, <a href="https://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, Imagery Â© <a href="https://www.mapbox.com/">Mapbox</a>',
    maxZoom: 18,
    id: 'mapbox.streets',
    accessToken: 'your.mapbox.access.token'
}).addTo(mymap);
var myIcon = L.icon({
	iconUrl: 'https://cdn3.iconfinder.com/data/icons/hotel-service-gray-set-1/100/Untitled-1-22-512.png',
	iconSize: [38, 38],
});

var marker = L.marker([`+lat+`, `+lng+`],{icon: myIcon,title:'Bag_24'}).addTo(mymap);
marker.bindPopup("<b>Bag_24</b>");
marker.on('mouseover', function (e) {
            this.openPopup();
        });
        marker.on('mouseout', function (e) {
            this.closePopup();
        });
</script>
</html>`
